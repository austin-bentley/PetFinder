<h1 class="animal-list__header">All Animals</h1>

<div class="animal-list__body">
    <%= render_many(
        @animals,
        PetFinderWeb.PartialsView,
        "animal_card.html",
        as: :animal,
        conn: @conn
    )%>

    <span><%= link "Back", to: Routes.page_path(@conn, :index), class: "animal-list__back-link" %></span>
</div>